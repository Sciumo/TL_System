let fun continue() = lex() in
  let fun scan (s,AcceptingLeaves : Internal.yyfinstate list list,l,i0) =
	let fun action (i,nil) = raise LexError
	| action (i,nil::l) = action (i-1,l)
	| action (i,(node::acts)::l) =
		case node of
		    Internal.N yyk => 
			(let val yytext = substring(!yyb,i0,i-i0)
			     val yypos = i0+ !yygone
			open UserDeclarations Internal.StartStates
 in (yybufpos := i; case yyk of 

			(* Application actions *)

  16 => ( SHELL(yytext           , yytext,     getNextTokenPos(yytext))  )
| 2 => ( getNextTokenPos(yytext); lex()                                     )
| 20 => ( SHELL(yytext           , yytext,     getNextTokenPos(yytext))  )
| 22 => ( SHELL(yytext           , yytext,     getNextTokenPos(yytext))  )
| 24 => ( SHELL(yytext           , yytext,     getNextTokenPos(yytext))  )
| 26 => ( SHELL(yytext           , yytext,     getNextTokenPos(yytext))  )
| 29 => ( SHELL(yytext           , yytext,     getNextTokenPos(yytext))  )
| 32 => ( YYBEGIN COMMENT; 
             			 comment_counter := !comment_counter + 1; 
						 getNextTokenPos(yytext); lex()                                 
						)
| 35 => ( comment_counter := !comment_counter + 1; 
					 	  getNextTokenPos(yytext); lex()
						)
| 38 => ( comment_counter := !comment_counter - 1; 
						  if !comment_counter = 0 then YYBEGIN INITIAL else (); 
						  getNextTokenPos(yytext); lex()      
                        )
| 40 => ( getNextTokenPos(yytext); lex()                                  )
| 42 => ( getNextTokenPos(yytext); lex()                                  )
| 51 => ( SHELL("file_id"    , yytext,     getNextTokenPos(yytext))  )
| 54 => ( SHELL("integer_value"        	   		, yytext, getNextTokenPos(yytext))  )
| 59 => ( SHELL("ident"        		   			, yytext, getNextTokenPos(yytext))  )
| 67 => ( SHELL(generateSchemaTokenName(yytext) 	, yytext, getNextTokenPos(yytext))  )
| 71 => ( SHELL(""                              	, yytext, getNextTokenPos(yytext))  )
| 75 => ( SHELL(""                              	, yytext, getNextTokenPos(yytext))  )
| 9 => ( SHELL(yytext           , yytext,     getNextTokenPos(yytext))  )
