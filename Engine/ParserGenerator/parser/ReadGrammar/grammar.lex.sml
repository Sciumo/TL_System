fun makeLexer yyinput =
let	val yygone0=1
	val yyb = ref "\n" 		(* buffer *)
	val yybl = ref 1		(*buffer length *)
	val yybufpos = ref 1		(* location of next character to use *)
	val yygone = ref yygone0	(* position in file of beginning of buffer *)
	val yydone = ref false		(* eof found yet? *)
	val yybegin = ref 1		(*Current 'start state' for lexer *)

	val YYBEGIN = fn (Internal.StartStates.STARTSTATE x) =>
		 yybegin := x

fun lex (yyarg as ({commentLevel, stringBuf, lastStart, sourceMap, issueMsg})) =
let fun continue() : Internal.result = 
  let fun scan (s,AcceptingLeaves : Internal.yyfinstate list list,l,i0) =
	let fun action (i,nil) = raise LexError
	| action (i,nil::l) = action (i-1,l)
	| action (i,(node::acts)::l) =
		case node of
		    Internal.N yyk => 
			(let val yytext = substring(!yyb,i0,i-i0)
			     val yypos = i0+ !yygone
			open UserDeclarations Internal.StartStates
 in (yybufpos := i; case yyk of 

			(* Application actions *)

  10 => ( Tokens.LPAREN(mkpos(yypos, yytext)) )
| 102 => ( commentLevel := !commentLevel + 0w1; continue() )
| 104 => ( SourceMap.newline(!sourceMap)(yypos); continue() )
| 106 => ( continue() )
| 108 => ( YYBEGIN INITIAL; mkstr(yyarg, true, Word.fromInt(yypos) + 0w1) )
| 110 => ( YYBEGIN INITIAL; SourceMap.newline(!sourceMap)(yypos); mkstr(yyarg, false, Word.fromInt yypos) )
| 12 => ( Tokens.RPAREN(mkpos(yypos, yytext)) )
| 129 => ( addChar(yyarg, Char.fromString(yytext), SOME{pos=mkpos(yypos, yytext), sev=WARNING, msg="illegal string escape ignored"}); continue() )
| 133 => ( YYBEGIN S_CONT; continue() )
| 136 => ( YYBEGIN S_CONT; SourceMap.newline(!sourceMap)(yypos+1); continue() )
| 138 => ( addChar(yyarg, SOME #"\\", NONE); issueMsg (mkpos(yypos, yytext)) WARNING "illegal string escape ignored" nullErrorBody; continue() )
| 14 => ( Tokens.LBRACK(mkpos(yypos, yytext)) )
| 140 => ( addChar(yyarg, SOME(String.sub(yytext, 0)), NONE); continue())
| 142 => ( issueMsg (mkpos(yypos, yytext)) WARNING "illegal non-printing character in string ignored" nullErrorBody; continue() )
| 144 => ( YYBEGIN STRING; continue())
| 147 => ( continue() )
| 149 => ( SourceMap.newline(!sourceMap)(yypos); continue() )
| 151 => ( YYBEGIN INITIAL; issueMsg (mkpos(yypos, yytext)) WARNING "unclosed string continuation sequence" nullErrorBody; mkstr(yyarg, true, Word.fromInt(yypos) + 0w1) )
| 153 => ( YYBEGIN STRING; addChar(yyarg, SOME(String.sub(yytext, 0)), NONE); issueMsg (mkpos(yypos, yytext)) WARNING "unclosed string continuation sequence" nullErrorBody; continue() )
| 155 => ( issueMsg (mkpos(yypos, yytext)) WARNING "illegal non-printing character in string continuation sequence ignored" nullErrorBody; continue() )
| 16 => ( Tokens.RBRACK(mkpos(yypos, yytext)) )
| 18 => ( Tokens.LBRACE(mkpos(yypos, yytext)) )
| 2 => ( continue() )
| 20 => ( Tokens.RBRACE(mkpos(yypos, yytext)) )
| 22 => ( Tokens.VBAR(mkpos(yypos, yytext)) )
| 24 => ( Tokens.ASTERISK(mkpos(yypos, yytext)) )
| 26 => ( Tokens.PLUS(mkpos(yypos, yytext)) )
| 28 => ( Tokens.QUESTION(mkpos(yypos, yytext)) )
| 30 => ( Tokens.END_PRODUCTION(mkpos(yypos, yytext)) )
| 33 => ( YYBEGIN COMMENT; commentLevel := 0w1; lastStart := Word.fromInt yypos; continue() )
| 36 => ( issueMsg (mkpos(yypos, yytext)) ERROR "unmatched close comment" nullErrorBody; continue() )
| 38 => ( YYBEGIN STRING; lastStart := Word.fromInt yypos; continue() )
| 4 => ( SourceMap.newline(!sourceMap)(yypos); continue() )
| 44 => ( Tokens.ID(mkpos'(yytext, yypos, yytext)) )
| 52 => ( Tokens.TID(mkpos'(String.substring(yytext, 1, String.size(yytext) - 2), yypos, yytext)) )
| 63 => ( Tokens.NON_ASSOC(mkpos(yypos, yytext)) )
| 75 => ( Tokens.LEFT_ASSOC(mkpos(yypos, yytext)) )
| 8 => ( Tokens.EQUALS(mkpos(yypos, yytext)) )
| 88 => ( Tokens.RIGHT_ASSOC(mkpos(yypos, yytext)) )
| 94 => ( Tokens.PREC(mkpos(yypos, yytext)) )
| 96 => ( issueMsg (mkpos(yypos, yytext)) ERROR "illegal token" nullErrorBody; continue() )
